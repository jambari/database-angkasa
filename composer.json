{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "anam/phantommagick": "^1.0",
        "backpack/backupmanager": "^1.1",
        "backpack/base": "^0.7.14",
        "backpack/crud": "^3.1",
        "backpack/langfilemanager": "^1.0",
        "backpack/logmanager": "^2.3",
        "backpack/pagemanager": "^1.1",
        "backpack/permissionmanager": "^2.1",
        "backpack/settings": "^2.0",
        "barryvdh/laravel-dompdf": "^0.7.1",
        "felixkiss/uniquewith-validator": "~2.0",
        "fx3costa/laravelchartjs": "^2.2",
        "hieu-le/laravel-alert": "~2.0",
        "laravel/framework": "5.3.*",
        "laravelcollective/html": "^5.3.0",
        "maatwebsite/excel": "~2.1.0",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "prologue/alerts": "^0.4.1",
        "yajra/laravel-datatables-oracle": "^6.0"
    },
    "require-dev": {
        "backpack/generators": "^1.1",
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
